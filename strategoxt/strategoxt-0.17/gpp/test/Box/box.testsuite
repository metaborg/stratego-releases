testsuite Box
options
  topsort BOX

test
  "H[]" -> H([],[])

test
  "V[]" -> V([],[])

test
  "" KW["if"] "" -> FBOX(KW,S("\"if\""))

test
  [ "if" ] -> S("\"if\"")

test
  "" HV[] "" -> HV([],[])

test
  "H hs=0 []" -> H([SOpt(HS,"0")],[])

test
  "V vs=0 []" -> V([SOpt(VS,"0")],[])

test
  "V is=0 vs=0 []" -> V([SOpt(IS,"0"),SOpt(VS,"0")],[])







