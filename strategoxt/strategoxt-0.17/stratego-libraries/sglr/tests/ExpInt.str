module ExpInt

signature
  constructors
    Minus : Exp * Exp -> Exp
    Plus  : Exp * Exp -> Exp
    Mod   : Exp * Exp -> Exp
    Div   : Exp * Exp -> Exp
    Mul   : Exp * Exp -> Exp
    Int   : IntConst -> Exp
          : String -> IntConst


signature
  constructors
    Some : a -> Option(a)
