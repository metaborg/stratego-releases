#!/bin/sh -v

# compiling C code generated by sc

prefix=/home/visser/res/xt
ATERM=/home/visser/res/xt
VERSION=0.4.18

gcc \
    -g \
    -I${prefix}/include \
    -I${ATERM}/include \
    -c $1.c

gcc \
    -g \
    $1.o -o $1 \
    -L${prefix}/lib -lstratego.dbg \
    -L${prefix}/lib -lstratego-lib.dbg \
    -L${ATERM}/lib -lATerm-dbg \
    -lm
